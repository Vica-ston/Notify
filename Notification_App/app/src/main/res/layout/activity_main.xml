<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="match_parent"
        android:layout_height="731dp"
        android:orientation="horizontal"
        app:layout_anchor="@+id/view_pager"
        app:layout_anchorGravity="top|center">

    </TableRow>

    <com.google.android.material.appbar.AppBarLayout
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:theme="@style/Theme.Notification_App.AppBarOverlay">

        <TextView
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:minHeight="?actionBarSize"
            android:padding="@dimen/appbar_padding"
            android:text="@string/app_name"
            android:textAppearance="@style/TextAppearance.Widget.AppCompat.Toolbar.Title" />

    </com.google.android.material.appbar.AppBarLayout>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/view_pager"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        app:layout_behavior="@string/appbar_scrolling_view_behavior" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="69dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="@dimen/fab_margin"
        android:layout_marginBottom="16dp"
        android:contentDescription="@string/tab_text_1"
        android:visibility="visible"
        app:srcCompat="@android:drawable/btn_star_big_on"
        tools:ignore="SpeakableTextPresentCheck"
        tools:visibility="visible" />

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="63dp"
        android:orientation="horizontal"
        app:layout_anchor="@+id/view_pager"
        app:layout_anchorGravity="center">

        <ImageButton
            android:id="@+id/imageButton3"
            style="@android:style/Widget.DeviceDefault.ImageButton"
            android:layout_width="82"
            android:layout_height="match_parent"
            android:accessibilityLiveRegion="none"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="SpeakableTextPresentCheck"
            tools:srcCompat="@drawable/steam_icon_logo_svg"
            tools:visibility="visible" />

        <ImageButton
            android:id="@+id/imageButton5"
            style="@android:style/Widget.DeviceDefault.ImageButton"
            android:layout_width="82"
            android:layout_height="match_parent"
            android:accessibilityLiveRegion="none"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="SpeakableTextPresentCheck"
            tools:srcCompat="@drawable/logo_twitter_icon_transparent_png"
            tools:visibility="visible" />

        <ImageButton
            android:id="@+id/imageButton6"
            style="@android:style/Widget.DeviceDefault.ImageButton"
            android:layout_width="82"
            android:layout_height="match_parent"
            android:accessibilityLiveRegion="none"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="SpeakableTextPresentCheck"
            tools:srcCompat="@drawable/_200px_instagram_icon"
            tools:visibility="visible" />

        <ImageButton
            android:id="@+id/imageButton7"
            style="@android:style/Widget.DeviceDefault.ImageButton"
            android:layout_width="82"
            android:layout_height="match_parent"
            android:accessibilityLiveRegion="none"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="SpeakableTextPresentCheck"
            tools:srcCompat="@drawable/sweaverflies_snapchat_icon"
            tools:visibility="visible" />

        <ImageButton
            android:id="@+id/imageButton8"
            style="@android:style/Widget.DeviceDefault.ImageButton"
            android:layout_width="82"
            android:layout_height="match_parent"
            android:accessibilityLiveRegion="none"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="SpeakableTextPresentCheck"
            tools:srcCompat="@drawable/facebook_icon_1"
            tools:visibility="visible" />
    </TableRow>

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="602dp"
        app:layout_anchor="@+id/tableRow2"
        app:layout_anchorGravity="bottom|center">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </ScrollView>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="601dp"
        app:layout_anchor="@+id/scrollView2"
        app:layout_anchorGravity="center">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:minHeight="50"
            android:paddingTop="6">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="57dp"
                android:layout_height="48dp"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="348dp"
                android:layout_height="wrap_content"
                android:text="John Doe has sent you a tweet! Click to see." />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:minHeight="50"
            android:paddingTop="6">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="57dp"
                android:layout_height="48dp"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Mary Jane has sent you a tweet! Click to see." />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:minHeight="50"
            android:paddingTop="6">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="57dp"
                android:layout_height="48dp"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Peter has sent you a tweet! Click to see." />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:minHeight="50"
            android:paddingTop="6">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="57dp"
                android:layout_height="48dp"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Joe has sent you a tweet! Click to see." />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:minHeight="50"
            android:paddingTop="6">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="57dp"
                android:layout_height="48dp"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="James has sent you a tweet! Click to see." />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="221dp"
            android:minHeight="50"
            android:paddingTop="6">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="57dp"
                android:layout_height="48dp"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Joseph has sent you a tweet! Click to see." />
        </TableRow>

    </TableLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>